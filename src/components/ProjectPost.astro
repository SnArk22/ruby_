---
const { frontmatter } = Astro.props;
const url = `../projects/${frontmatter.slug}`;
---
<li class="project-preview">
    <a href={url}>
        <img src={frontmatter.image.url} alt={frontmatter.image.alt} class='Project_image'/>
        <h3>{frontmatter.title}</h3>
    </a>
    <p> Published: {frontmatter.pubDate}</p>
    
</li>